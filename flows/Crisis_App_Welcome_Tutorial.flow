            <extensionName>flowruntime:image</extensionName>
            <fieldType>ComponentInstance</fieldType>
            <inputParameters>
                <name>imageName</name>
                <value>
                    <stringValue>Crisis_Welcome_BusinessImpact1</stringValue>
                </value>
            </inputParameters>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>businessimpact2</name>
            <fieldText>&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;Click &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;New&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; next to the &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Locations&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; list. Add the location you work at.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;You can also click on the &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Locations &lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;tab at the very top of the screen to manage all of your locations. &lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Inside of a location record, you can: &lt;/b&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;Schedule Cleanings&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;Manage Repairs&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;Collaborate Around Closures &amp;amp; Re-Openings&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;  &lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;Click &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Next&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; to continue&lt;/span&gt;&lt;/p&gt;</fieldText>
