        <fields>
            <name>LetsStart</name>
            <fieldText>&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; A &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Crisis&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; record is where you&apos;ll manage everything about an ongoing crisis:&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; &lt;/span&gt;&lt;i style=&quot;font-size: 18px;&quot;&gt;How many people are impacted&lt;/i&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;i style=&quot;font-size: 18px;&quot;&gt;How many locations are impacted&lt;/i&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;i style=&quot;font-size: 18px;&quot;&gt;How many people we&apos;ve helped with assistance programs&lt;/i&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;i style=&quot;font-size: 18px;&quot;&gt;News we&apos;ve published to the employee portal&lt;/i&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px; color: rgb(0, 0, 0); background-color: rgb(255, 255, 255);&quot;&gt;Let&apos;s start by creating a &lt;/span&gt;&lt;b style=&quot;font-size: 18px; color: rgb(0, 0, 0); background-color: rgb(255, 255, 255);&quot;&gt;Crisis&lt;/b&gt;&lt;span style=&quot;font-size: 18px; color: rgb(0, 0, 0); background-color: rgb(255, 255, 255);&quot;&gt; record:&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>NameOfCrisis</name>
            <dataType>String</dataType>
            <fieldText>What is the name of the crisis you want to create?</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>Description</name>
            <dataType>String</dataType>
            <fieldText>How would you describe this in a little more text?</fieldText>
            <fieldType>InputField</fieldType>
            <isRequired>false</isRequired>
        </fields>
        <fields>
            <name>What_type_of_crisis_is_this</name>
            <choiceReferences>CrisisTypePicklist</choiceReferences>
            <dataType>String</dataType>
            <fieldText>What type of crisis is this?</fieldText>
            <fieldType>DropdownBox</fieldType>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>ExplainCrisisMore</name>
            <fieldText>&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;Once created, you&apos;ll be able to access the crisis record by clicking on the &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Crises&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; tab up top. &lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;&lt;span class=&quot;ql-cursor&quot;&gt;﻿&lt;/span&gt;Here is what your record will look like:&lt;/span&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>Crisis_Welcome_Impact1</name>
            <extensionName>flowruntime:image</extensionName>
            <fieldType>ComponentInstance</fieldType>
            <inputParameters>
                <name>imageName</name>
                <value>
                    <stringValue>Crisis_Welcome_Crisis1</stringValue>
                </value>
            </inputParameters>
            <isRequired>true</isRequired>
        </fields>
        <fields>
            <name>clickonyouronw</name>
            <fieldText>&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;Click on the &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Crises&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; tab, then click the drop down on the top-left of your screen where it says &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Recently Viewed&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;. &lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;﻿Click the &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;All&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; list to see your crisis record.&lt;/span&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;br&gt;&lt;/p&gt;&lt;p style=&quot;text-align: center;&quot;&gt;&lt;b style=&quot;font-size: 18px; background-color: rgb(255, 255, 255); color: rgb(0, 0, 0);&quot;&gt;Click on your new Crisis record. &lt;/b&gt;&lt;/p&gt;</fieldText>
            <fieldType>DisplayText</fieldType>
        </fields>
        <fields>
            <name>ClickNextCrisis</name>
            <fieldText>&lt;p style=&quot;text-align: center;&quot;&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt;Then, click &lt;/span&gt;&lt;b style=&quot;font-size: 18px;&quot;&gt;Next&lt;/b&gt;&lt;span style=&quot;font-size: 18px;&quot;&gt; to continue.&lt;/span&gt;&lt;/p&gt;</fieldText>
